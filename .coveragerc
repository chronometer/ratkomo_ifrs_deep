[run]
source = src
omit =
    src/__init__.py
    src/config/__init__.py
    src/agents/__init__.py
    src/orchestration/__init__.py
    tests/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
    
[html]
directory = coverage_html

[xml]
output = coverage.xml
